issn = {13652699},
journal = {Journal of Biogeography},
keywords = {Amazon Basin,Andes uplift,Aves,Cryptic speciation,Drainage capture,Historical biogeography,Neotropics,River barriers,River refuge hypothesis,Thamnophilidae},
number = {6},
pages = {1094--1104},
title = {{Multiple speciation across the Andes and throughout Amazonia: The case of the spot-backed antbird species complex (\textit{Hylophylax naevius}/\textit{Hylophylax naevioides})}},
