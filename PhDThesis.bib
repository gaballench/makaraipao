journal = {Proceedings of the Royal Society B},
keywords = {Animals,Ecosystem,Genetic Speciation,Passeriformes,Passeriformes: genetics,Phylogeny,South America},
number = {1639},
pages = {1133--42},
pmid = {18285279},
title = {{Out of Amazonia again and again: episodic crossing of the Andes promotes diversification in a lowland forest flycatcher.}},
