isbn = {1471-2970 (Electronic)$\backslash$r0962-8436 (Linking)},
issn = {0962-8436},
journal = {Philosophical Transactions of the Royal Society B},
keywords = {evolution,palaeontology,systematics,taxonomy and,theoretical biology},
number = {20150129},
pages = {1--14},
pmid = {27325827},
title = {{Bayesian phylogenetic estimation of fossil ages}},
url = {http://arxiv.org/abs/1601.07447},
volume = {371},
