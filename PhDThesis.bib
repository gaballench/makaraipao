issn = {09442006},
journal = {Zoology},
keywords = {Adductor mandibulae,Characiformes,Fish musculature,Myology},
number = {2},
pages = {84--116},
pmid = {22445572},
publisher = {Elsevier GmbH.},
title = {{Anatomy and evolution of the mandibular, hyopalatine, and opercular muscles in characiform fishes (Teleostei: Ostariophysi)}},
