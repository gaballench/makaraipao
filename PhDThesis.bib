doi = {10.1643/CI-12-013},
file = {:home/balleng/Dropbox/Gustavo/Mendeley/Stewart - 2013 - Re-description of iArapaima agassiziii (Valenciennes), a rare fish from Brazil (Osteoglossomorpha Osteoglossidae).pdf:pdf},
