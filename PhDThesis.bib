doi = {10.1643/CI-04-036R1},
file = {:home/balleng/Dropbox/Gustavo/Mendeley/Lundberg, Akama - 2005 - iBrachyplatystoma capapretumi a new species of Goliath Catfish from the Amazon basin, with a reclassification o.pdf:pdf},
