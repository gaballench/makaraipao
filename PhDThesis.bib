file = {:home/balleng/Dropbox/Gustavo/Mendeley/Andrade et al. - 2017 - A new species of iTometesi Valenciennes 1850 (Characiformes Serrasalmidae) from Tocantins-Araguaia River Basin b.pdf:pdf},
isbn = {1111111111},
issn = {19326203},
journal = {PLoS ONE},
number = {4},
title = {{A new species of \textit{Tometes} Valenciennes 1850 (Characiformes: Serrasalmidae) from Tocantins-Araguaia River Basin based on integrative analysis of molecular and morphological data}},
volume = {12},
