number = {1},
pages = {133--142},
publisher = {Taylor {\&} Francis},
title = {{First timetree of Sphyraenidae (Percomorpha) reveals a Middle Eocene crown age and an Oligo–Miocene radiation of Barracudas}},
url = {http://dx.doi.org/10.1080/11250003.2014.962630},
volume = {82},
year = {2015}
}
@article{Cheviron2005,
abstract = {Here we apply a combination of phylogeographic and historical demographic analyses to the study of mtDNA sequence variation within the Blue-crowned Manakin (Lepidothrix coronata), a widespread Neotropical bird. A high degree of phylogeographic structure allowed us to demonstrate that several vicariant events, including Andean uplift, the formation of riverine barriers, and climatically induced vegetational shifts, as well as a non-vicariant process, range expansion, have all acted, at varying spatial and temporal scales, to influence genetic structure within L. coronata, suggesting that current historical hypotheses of the origin of Neotropical avian diversity that focus on single vicariant mechanisms may be overly simplistic. Our data also support an origin ({\textgreater}2 mybp) that is substantially older than the late Pleistocene for the genetic structure within this species and indicate that phylogeographic patterns within the species are not concordant with plumage-based subspecific taxonomy. These data add to a growing body of evidence suggesting that the origin of several Neotropical avian species may have occurred in the mid-Pliocene, thus, geological arguments surrounding putative Pleistocene vicariant events, while interesting in their own right, may have little relevance to Neotropical avian diversification at the species level. ?? 2005 Elsevier Inc. All rights reserved.},
author = {Cheviron, Z. A. and Hackett, Shannon J. and Capparella, Angelo P.},
