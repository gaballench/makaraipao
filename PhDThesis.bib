journal = {Geological Society Special Publication},
keywords = {computer aided design,geodynamics,geostatistics,mathematics,modeling,visualization},
number = {June 2007},
pages = {59--97},
title = {{From graphical display to dynamic model: Mathematical geology in the Earth sciences in the nineteenth and twentieth centuries}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0036232346{\&}partnerID=40{\&}md5=c182c091960346a6e8fdaf44b27af09f},
