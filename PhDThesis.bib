isbn = {1055-7903},
issn = {10557903},
journal = {Molecular Phylogenetics and Evolution},
keywords = {Andean orogeny,Boraginaceae,Neotropics,Tournefortia},
number = {1},
pages = {90--102},
pmid = {21689768},
publisher = {Elsevier Inc.},
title = {{Diversification in the Andes: Age and origins of South American \textit{Heliotropium} lineages (Heliotropiaceae, Boraginales)}},
