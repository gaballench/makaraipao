journal = {Geobios},
keywords = {biogeography,conglomerado os{\'{i}}fero,ground sloth,mesopotamic region,taxonomy},
number = {1},
pages = {33--44},
publisher = {Elsevier Masson SAS},
title = {{The Megalonychidae (Xenarthra, Tardigrada) from the late Miocene of Entre R{\'{i}}os Province, Argentina, with remarks on their systematics and biogeography}},
