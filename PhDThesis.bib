journal = {Comptes Rendus de l'Academie des Sciences - Serie III},
keywords = {Amapa,Amazonia,Biogeography,Freshwater fishes,Guianas,Serrasalminae,Varzea},
number = {12},
pages = {1133--1143},
title = {{Lower Oyapock River as northern limit for the Western Amazon fish fauna or only a stage in its northward progression}},
