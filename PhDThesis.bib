issn = {10557903},
journal = {Molecular Phylogenetics and Evolution},
keywords = {Amazonia,Historical demography,Lepidothrix,Neotropics,Nested clade analysis,Phylogeography,Refugia,Vicariant events},
number = {2},
pages = {338--357},
pmid = {15955514},
title = {{Complex evolutionary history of a Neotropical lowland forest bird (\textit{Lepidothrix coronata}) and its implications for historical hypotheses of the origin of Neotropical avian diversity}},
