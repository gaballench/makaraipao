file = {:home/balleng/Dropbox/Gustavo/Mendeley/Venegas-Anaya et al. - 2008 - Mitochondrial DNA phylogeography of Caiman crocodilus in MesoAmerica and South America.pdf:pdf},
isbn = {1932-5231},
issn = {19325223},
journal = {Journal of Experimental Zoology Part A: Ecological Genetics and Physiology},
number = {10},
pages = {614--627},
pmid = {18831056},
title = {{Mitochondrial DNA phylogeography of Caiman crocodilus in MesoAmerica and South America}},
volume = {309},
year = {2008}
}
