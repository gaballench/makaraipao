file = {:home/balleng/Dropbox/Gustavo/Mendeley/Carrillo-Brice{\~{n}}o et al. - 2019 - Neogene Caribbean elasmobranchs Diversity, paleoecology and paleoenvironmental significance of the Coc.pdf:pdf},
issn = {17264189},
journal = {Biogeosciences},
number = {1},
pages = {33--56},
title = {{Neogene Caribbean elasmobranchs: Diversity, paleoecology and paleoenvironmental significance of the Cocinetas Basin assemblage (Guajira Peninsula, Colombia)}},
volume = {16},
