isbn = {0000000299},
issn = {14712954},
journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {Bayesian phylogenetics,Divergence times estimation,Fossil data,Fossilized birth-death,Stratigraphic age uncertainty},
number = {1902},
pages = {20190685},
title = {{Ignoring stratigraphic age uncertainty leads to erroneous estimates of species divergence times under the fossilized birth-death process}},
volume = {286},
year = {2019}
}
