journal = {Journal of Archaeological Science: Reports},
keywords = {Catfishes,Maya,Osteometry,Pre-Hispanic fisheries},
number = {2016},
pages = {116--120},
publisher = {Elsevier Ltd},
title = {{Estimation of fish size from archaeological bones of hardhead catfishes (\textit{Ariopsis felis}): Assessing pre-Hispanic fish acquisition of two Mayan sites}},
